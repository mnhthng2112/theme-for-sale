/* Tooltips */
.gen-tooltip {
  position: relative;
}
.gen-tooltip .gen-tooltip__content {
  position: absolute;
  background: #1f1f27;
  border-radius: 4px;
  min-width: -moz-max-content;
  min-width: max-content;
  color: #fff;
  font-size: 13px;
  line-height: 16px;
  padding: 6px 8px;
  opacity: 0;
  visibility: hidden;
  transition: 0.25s all;
}
.gen-tooltip .gen-tooltip__content::before {
  content: "";
  width: 6px;
  height: 6px;
  position: absolute;
  background: #1f1f27;
  transform: rotate(45deg);
}
.gen-tooltip.gen-tooltip--top .gen-tooltip__content {
  bottom: calc(100% + 12px);
  left: 50%;
  transform: translateX(-50%);
}
.gen-tooltip.gen-tooltip--top .gen-tooltip__content::before {
  left: calc(50% - 3px);
  bottom: -3px;
}
.gen-tooltip.gen-tooltip--bottom .gen-tooltip__content {
  top: calc(100% + 12px);
  left: 50%;
  transform: translateX(-50%);
}
.gen-tooltip.gen-tooltip--bottom .gen-tooltip__content::before {
  left: calc(50% - 3px);
  top: -3px;
}
.gen-tooltip.gen-tooltip--left .gen-tooltip__content {
  right: calc(100% + 12px);
  top: 50%;
  transform: translateY(-50%);
}
.gen-tooltip.gen-tooltip--left .gen-tooltip__content::before {
  top: calc(50% - 3px);
  right: -3px;
}
.gen-tooltip.gen-tooltip--right .gen-tooltip__content {
  left: calc(100% + 12px);
  top: 50%;
  transform: translateY(-50%);
}
.gen-tooltip.gen-tooltip--right .gen-tooltip__content::before {
  top: calc(50% - 3px);
  left: -3px;
}
.gen-tooltip:hover .gen-tooltip__content {
  opacity: 1;
  visibility: visible;
}
.gen-tooltip:hover.gen-tooltip--top .gen-tooltip__content {
  bottom: calc(100% + 6px);
}
.gen-tooltip:hover.gen-tooltip--bottom .gen-tooltip__content {
  top: calc(100% + 6px);
}
.gen-tooltip:hover.gen-tooltip--left .gen-tooltip__content {
  right: calc(100% + 6px);
}
.gen-tooltip:hover.gen-tooltip--right .gen-tooltip__content {
  left: calc(100% + 6px);
}
/* Responsive image */
[style*="--aspect-ratio"] {
  position: relative;
  overflow: hidden;
}
[style*="--aspect-ratio"]::after {
  content: "";
  display: block;
  width: 100%;
  height: auto;
